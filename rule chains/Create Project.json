          "tbelScript": "var m = JSON.parse(metadata.measurementData);\n\nvar attributes = {\n    progress: 'in preparation',\n    state: 'normal',\n    installationType: m.installationType,\n    systemType: m.systemType,\n    measurementType: m.measurementType,\n    measurementRole: m.measurementRole,\n    hydraulicScheme: m.hydraulicScheme,\n    fluidType: m.fluidType,\n    designFlowTemp: m.designFlowTemp,\n    designReturnTemp: m.designReturnTemp,\n    designDeltaT: m.designDeltaT,\n    designPower: m.designPower,\n    designFlow: m.designFlow,\n    pipeDimension: m.pipeDimension,\n    valveDimension: m.valveDimension,\n    valveKvs: m.valveKvs,\n    flowOnThreshold: m.flowOnThreshold,\n    hysteresisMinutes: m.hysteresisMinutes,\n    weeklySchedule: m.weeklySchedule,\n    collapseThreshold: m.collapseThreshold,\n    spikeThreshold: m.spikeThreshold,\n    cyclingThreshold: m.cyclingThreshold,\n    stabilityThreshold: m.stabilityThreshold,\n    pumpPresent: m.pumpPresent,\n    pumpControlType: m.pumpControlType,\n    pumpRatedPower: m.pumpRatedPower,\n    auxSensor1: m.auxSensor1,\n    auxSensor2: m.auxSensor2,\n    calculatePower: m.calculatePower\n};\n\n// Remove null values\nvar clean = {};\nvar keys = attributes.keys();\nforeach(k : keys) {\n    if (attributes[k] != null) {\n        clean[k] = attributes[k];\n    }\n}\n\nreturn { msg: clean, metadata: metadata, msgType: msgType };"
