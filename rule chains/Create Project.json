      {
        "type": "org.thingsboard.rule.engine.transform.TbTransformMsgNode",
        "name": "Generate Project Name",
        "debugSettings": { "allEnabled": true },
        "singletonMode": false,
        "queueName": null,
        "configurationVersion": 0,
        "configuration": {
          "scriptLang": "TBEL",
          "jsScript": "",
          "tbelScript": "// msg contains paginated response\nvar projects = msg.data;\nvar maxNum = 0;\n\n// Find highest project number\nforeach(p : projects) {\n    var name = p.name;\n    var parts = name.split('_P-');\n    if (parts.length() == 2) {\n        var numStr = parts[1];\n        var num = parseInt(numStr);\n        if (num > maxNum) {\n            maxNum = num;\n        }\n    }\n}\n\nvar nextNum = maxNum + 1;\nvar paddedNum = 'P-' + ('0000' + nextNum).substring(String(nextNum).length());\n\n// Use customer shortName or fallback\nvar shortName = metadata.ss_shortName;\nif (shortName == null || shortName == '') {\n    shortName = 'PRJ';\n}\n\nvar entityName = shortName + '_' + paddedNum;\nmetadata.projectEntityName = entityName;\n\n// Prepare Project asset body\nvar projectBody = {\n    name: entityName,\n    type: 'Project',\n    label: metadata.projectName,\n    customerId: {\n        entityType: 'CUSTOMER',\n        id: metadata.customerId\n    }\n};\n\nreturn { msg: projectBody, metadata: metadata, msgType: msgType };"
        },
        "additionalInfo": {
          "description": "Generate unique project entityName",
          "layoutX": 1500,
          "layoutY": 200
        }
