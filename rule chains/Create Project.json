        "singletonMode": false,
        "queueName": null,
        "configurationVersion": 0,
        "configuration": {
          "scriptLang": "TBEL",
          "jsScript": "",
          "tbelScript": "// Store ALL data in metadata (msg will be overwritten by API responses)\nvar projectData = msg.createProject;\n\n// Store project data\nmetadata.projectName = projectData.projectName;\nmetadata.address = projectData.address;\nmetadata.postalCode = projectData.postalCode;\nmetadata.city = projectData.city;\nmetadata.country = projectData.country;\nmetadata.latitude = String(projectData.latitude);\nmetadata.longitude = String(projectData.longitude);\nmetadata.normOutdoorTemp = String(projectData.normOutdoorTemp);\nmetadata.units = projectData.units;\n\n// Store area as JSON string\nif (projectData.area != null) {\n    metadata.area = JSON.stringify(projectData.area);\n}\n\n// Store measurements as JSON string\nif (projectData.measurements != null) {\n    metadata.measurementsJson = JSON.stringify(projectData.measurements);\n    metadata.measurementCount = String(projectData.measurements.length());\n} else {\n    metadata.measurementsJson = '[]';\n    metadata.measurementCount = '0';\n}\n\n// Store customer info\nmetadata.customerId = metadata.originatorId;\nmetadata.customerType = metadata.originatorType;\n\n// Prepare login body - REPLACE WITH ACTUAL CREDENTIALS\nvar loginBody = {\n    username: 'api-user@ecoenergygroup.com',\n    password: 'API_PASSWORD_HERE'\n};\n\nreturn { msg: loginBody, metadata: metadata, msgType: msgType };"
        },
        "additionalInfo": {
