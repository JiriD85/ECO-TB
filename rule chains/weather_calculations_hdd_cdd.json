        "configuration": {
          "scriptLang": "TBEL",
          "jsScript": "var temp = msg.outsideTemp;\nif (temp === null || temp === undefined) {\n    return { msg: msg, metadata: metadata, msgType: msgType };\n}\n\nvar baseTemp = metadata.ss_base_temp ? parseFloat(metadata.ss_base_temp) : 18.0;\nvar hdd = Math.max(0, baseTemp - temp);\nvar cdd = Math.max(0, temp - baseTemp);\n\nmsg.hdd = hdd;\nmsg.cdd = cdd;\n\nreturn { msg: msg, metadata: metadata, msgType: msgType };",
          "tbelScript": "var temp = msg.outsideTemp;\nif (temp === null || temp === undefined) {\n    return { msg: msg, metadata: metadata, msgType: msgType };\n}\n\nvar baseTemp = metadata.ss_base_temp ? parseFloat(metadata.ss_base_temp) : 18.0;\nvar hdd = Math.max(0, baseTemp - temp);\nvar cdd = Math.max(0, temp - baseTemp);\n\nmsg.hdd = hdd;\nmsg.cdd = cdd;\n\nreturn { msg: msg, metadata: metadata, msgType: msgType };"
        },
